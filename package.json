{
  "name": "react-typescript-tooling-automation-template-vite",
  "description": "A React template with TypeScript, developer tooling and automation.",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc",
    "lint": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:write": "npm run lint -- --fix",
    "lint:ls": "ls-lint",
    "format": "prettier --check \"./**/*.{js,jsx,ts,tsx,html,json,md}\"",
    "format:write": "prettier --write \"./**/*.{js,jsx,ts,tsx,html,json,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.10.1",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.11.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.0",
    "jsdom": "^20.0.1",
    "ls-lint": "^0.1.2",
    "msw": "^0.47.4",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-checker": "^0.5.1",
    "vitest": "^0.23.4",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
